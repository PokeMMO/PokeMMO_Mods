		<area name="pcslot.bg.default" xywh="3,1,80,80"/>
		<area name="pcslot.bg.active" xywh="86,1,80,80"/>
		<area name="pcslot.inner.default" xywh="3,83,80,80"/>
		<area name="pcslot.inner.active" xywh="86,83,80,80"/>
		<area name="pcslot.inner.filtered" xywh="169,83,80,80"/>
		<area name="pcslot.bg.overlay.default" xywh="86,164,80,80"/>
		<area name="pcslot.bg.overlay.disabled" xywh="3,164,80,80"/>
		<area name="pcslot.bg.overlay.battlebox" xywh="169,164,80,80"/>
		<area name="pcslot.border.default" xywh="3,247,80,80"/>
		<area name="pcslot.border.selected" xywh="86,247,80,80"/>
		<area name="pcslot.border.selected2" xywh="86,247,80,80" inset="-1" />
		<area name="pcslot.border.filtered" xywh="169,247,80,80"/>
    <area name="pcslot.border.filtered2" xywh="169,247,80,80" inset="-1"/>
		<composed name="pc-slot.background">
			<!-- BG Layer -->
			<select center="false">
				<alias ref="pcslot.bg.active" if="filtered"/>
				<alias ref="pcslot.bg.default" />
			</select>
			<!-- BG Overlay Layer -->
			<select center="false">
				<alias ref="pcslot.bg.overlay.disabled" if="obey"/>
        <alias ref="pcslot.bg.overlay.battlebox" if="battlebox"/>
<!-- 				<alias ref="pcslot.bg.overlay.default" /> -->
			</select>
			<!-- Inner Layer -->
			<select center="false">
				<alias ref="pcslot.inner.filtered" if="selected + filtered"/>
				<alias ref="pcslot.inner.active" if="selected"/>
				<alias ref="pcslot.inner.default" />
			</select>
			<!-- Border Layer -->
      <select center="false">
        <alias if="multiselectActive" ref="pcslot.border.filtered"/>
        <animation if="multiselect" timeSource="keyboardFocus">
          <frame duration="400" ref="pcslot.border.filtered2" tint="#FFFFFFFF"/>
          <frame duration="400" ref="pcslot.border.filtered" tint="#FFFFFFFF"/>
        </animation>
        <animation if="keyboardFocus" timeSource="keyboardFocus">
          <frame duration="400" ref="pcslot.border.selected2" tint="#FFFFFFFF"/>
          <frame duration="400" ref="pcslot.border.selected" tint="#FFFFFFFF"/>
        </animation>
        <alias ref="pcslot.border.default"/>
      </select>
			<select center="false">
				<alias ref="pcslot.border.filtered" inset="2" if="filtered"/>
			</select>
		</composed>

    <select name="pc-slot-border-while-selected.background">
      <alias ref="pcslot.border.selected" if="keyboardFocus"/>
    </select>
