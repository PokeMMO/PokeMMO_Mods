<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<theme name="monster-frame-mobile" ref="resizableframe">
		<param name="fadeDurationShow"><int>200</int></param>
		<param name="fadeDurationHide"><int>1</int></param>
		<param name="background"><image>mobile.window.grid.background</image></param>
		<param name="border"><border>0,0,0,0</border></param>

		<param name="maxWidth"><int>439</int></param>
		<param name="maxHeight"><int>340</int></param>

		<param name="titleAreaTop"><int>30</int></param>
		<param name="titleAreaLeft"><int>20</int></param>
		<param name="titleAreaRight"><int>0</int></param>
		<param name="titleAreaBottom"><int>0</int></param>
		<theme name="title" ref="-defaults">
			<param name="textAlignment"><enum type="alignment">left</enum></param>
			<param name="font"><font>xxlarge</font></param>
		</theme>

		<param name="closeButtonX"><int>-64</int></param>
		<param name="closeButtonY"><int>2</int></param>
		<theme name="closeButton" ref="-defaults">
			<param name="background"><image>mobile.icons.close</image></param>
			<param name="minWidth"><int>60</int></param>
			<param name="minHeight"><int>60</int></param>
		</theme>

		<!-- Used for Name Section -->
		<theme name="nameplate" ref="-defaults">
			<param name="minWidth"><int>340</int></param>
			<param name="minHeight"><int>34</int></param>
			<param name="maxHeight"><int>34</int></param>
			<param name="border"><border>2,3</border></param>
