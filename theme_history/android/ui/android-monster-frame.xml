        <param name="font"><font>med-shadow-22-white</font></param>
        <param name="textAlignment"><enum type="alignment">LEFT</enum></param>
      </theme>
      <theme name="label-monster-name" ref="-defaults">
        <param name="font"><font>med-shadow-22-white</font></param>
        <param name="textAlignment"><enum type="alignment">LEFT</enum></param>
        <param name="minWidth"><int>250</int></param>
        <param name="maxWidth"><int>250</int></param>
      </theme>
      <param name="defaultGap"><dimension>2,0</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>

		<!-- Used for Item Held section -->
		<theme name="itemplate" ref="-defaults">
			<param name="background"><image>none</image></param>
			<param name="minHeight"><int>34</int></param>
			<param name="maxHeight"><int>34</int></param>
			<param name="border"><border>10,22</border></param>
			<theme name="label" ref="-defaults">
				<param name="font"><font>med-shadow-22-white</font></param>
				<param name="textAlignment"><enum type="alignment">LEFT</enum></param>
			</theme>
			<theme name="label2" ref="-defaults">
				<param name="font"><font>18pt-gray</font></param>
				<param name="background"><image>mobile.label.value</image></param>
        <param name="border"><border>0,15,1,60</border></param>
        <param name="minWidth"><int>240</int></param>
			</theme>
			<param name="defaultGap"><dimension>2,0</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>

		<theme name="tabbedpane" ref="-defaults">
				<param name="minWidth"><int>245</int></param>
				<param name="background"><image>none</image></param>
				<param name="border"><border>0,0,0,0</border></param>
				<theme name="tabbox" ref="-defaults">
					<!-- Not used, look below for custom tab buttons -->
					<theme name="tabbutton" ref="togglebutton">
						<param name=""><image>ui-tab-button.*</image></param>
					</theme>
					<param name="border"><border>0,350,0,6</border></param>
					<param name="spacing"><int>5</int></param>
					<param name="alignment"><enum type="alignment">FILL</enum></param>
				</theme>
				<theme name="container" ref="-defaults">
					<param name="border"><border>0,0,0,0</border></param>
          <theme name="dialoglayout" ref="-defaults">
            <param name="minWidth"><int>800</int></param>
            <param name="background"><image>none</image></param>
						<param name="border"><border>0</border></param>
						<param name="defaultGap"><dimension>5,5</dimension></param>
						<param name="smallGap"><dimension>8,8</dimension></param>
						<param name="mediumGap"><dimension>10,10</dimension></param>
						<param name="largeGap"><dimension>15,15</dimension></param>
						<param name="namedGaps"><map/></param>
					</theme>
          <theme name="scrollpane" ref="scrollpane">
            <param name="minWidth"><int>800</int></param>
            <param name="background"><image>none</image></param>
            <param name="border"><border>0</border></param>
            <param name="defaultGap"><dimension>5,5</dimension></param>
            <param name="smallGap"><dimension>8,8</dimension></param>
            <param name="mediumGap"><dimension>10,10</dimension></param>
            <param name="largeGap"><dimension>15,15</dimension></param>
            <param name="namedGaps"><map/></param>
            <theme name="dialoglayout" ref="dialoglayout">
              <param name="border"><border>0</border></param>
            </theme>
          </theme>
				</theme>
				<param name="inputMap"><inputMapDef>
					<action name="nextTab">CTRL TAB</action>
					<action name="prevTab">CTRL SHIFT TAB</action>
				</inputMapDef></param>
			</theme>
	</theme>

