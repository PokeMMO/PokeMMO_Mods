<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<!-- guild window -->
	<theme name="guildwindow" ref="mobile-panel" overwrite="true">
		<param name="border"><border>0</border></param>
		<param name="fadeDurationShow"><int>200</int></param>
		<param name="fadeDurationHide"><int>200</int></param>

		<theme name="settings-frame" ref="mobile-panel" overwrite="true">
			<theme name="changemotd" ref="-defaults" allowWildcard="true">
				<param name="background"><image>none</image></param>
				<param name="border"><border>75,15,0,15</border></param>
				<param name="defaultGap"><dimension>5,5</dimension></param>
				<param name="smallGap"><dimension>8,8</dimension></param>
				<param name="mediumGap"><dimension>10,10</dimension></param>
				<param name="largeGap"><dimension>15,15</dimension></param>
				<param name="namedGaps"><map/></param>
				<param name="maxWidth"><int>150</int></param>
				<param name="maxHeight"><int>150</int></param>
				<theme name="scrollpane">
					<param name="background"><image>none</image></param>
					<param name="overlay"><image>none</image></param>
					<theme name="hscrollbar" ref="hscrollbar"/>  <!-- not used -->
					<theme name="vscrollbar" ref="vscrollbar-white"/>
					<param name="border"><border>0,0,0,0</border></param>

					<param name="maxWidth"><int>32767</int></param>
					<param name="maxHeight"><int>32767</int></param>
					<param name="hasDragButton"><bool>false</bool></param>
					<param name="autoScrollArea"><int>5</int></param>
					<param name="autoScrollSpeed"><int>10</int></param>
					<param name="minHeight"><int>170</int></param>
					<theme name="editfield" ref="editfield" allowWildcard="true">
						<param name="background"><image>ui-inputbox.background</image></param>
						<param name="font"><font>med-noshadow-18-white</font></param>
						<theme name="renderer" ref="-input">
							<param name="font"><font>med-noshadow-18-white</font></param>
						</theme>
						<param name="border"><border>3</border></param>
						<param name="passwordChar"><int>35</int></param>
						<param name="maxWidth"><int>32767</int></param>
						<param name="columns"><int>25</int></param>
						<param name="mouseCursor"><cursor>cursor.text</cursor></param>
						<param name="autocompletion-height"><int>250</int></param>
						<param name="minWidth"><int>600</int></param>
					</theme>
				</theme>
				<theme name="button" ref="mobile-bottom-button" overwrite="true">
					<param name="minWidth"><int>150</int></param>
				</theme>
			</theme>
		</theme>
		<theme name="dialoglayout" ref="dialoglayout">
			<param name="border"><border>0</border></param>
			<theme name="tabbedpane" ref="base-tabbed-pane">
				<param name="tabPosition"><enum type="tabposition">LEFT</enum></param>
				<param name="background"><image>none</image></param>
				<theme name="tabbox" ref="mobile-tabbox-inset" overwrite="true">
					<param name="border"><border>60,0,0,0</border></param>
					<theme name="tabbutton" ref="tabbutton">
						<param name="minWidth"><int>208</int></param>
					</theme>
				</theme>
				<theme name="container" ref="container">
					<param name="border"><border>0,10,0,10</border></param>
					<param name="minWidth"><int>650</int></param>
					<theme name="dialoglayout" ref="dialoglayout">
						<param name="border"><border>0</border></param>
						<theme name="label" ref="label" overwrite="true">
							<param name="border"><border>0,0</border></param>
							<param name="font"><font>med-noshadow-18-white</font></param>
						</theme>
						<theme name="button" ref="mobile-bottom-button" overwrite="true">
							<param name="border"><border>0,0,0,0</border></param>
							<param name="minWidth"><int>150</int></param>
						</theme>
						<theme name="editfield" ref="editfield-fancy"/>

						<theme name="motdscroll">
							<param name="background"><image>none</image></param>
							<param name="overlay"><image>none</image></param>
							<theme name="hscrollbar" ref="hscrollbar"/>  <!-- not used -->
							<theme name="vscrollbar" ref="vscrollbar-white"/>
							<param name="border"><border>3</border></param>

							<param name="maxWidth"><int>32767</int></param>
							<param name="maxHeight"><int>32767</int></param>
							<param name="hasDragButton"><bool>false</bool></param>
							<param name="autoScrollArea"><int>5</int></param>
							<param name="autoScrollSpeed"><int>10</int></param>
							<param name="scrollbarsAlwaysVisible"><bool>false</bool></param>
							<param name="minWidth"><int>450</int></param>
							<param name="minHeight"><int>1</int></param>
							<!-- message of the day -->
							<theme name="textarea" ref="-defaults">
								<param name="background"><image>none</image></param>
								<param name="minWidth"><int>450</int></param>
								<param name="fonts"><map>
									<param name="default"><font>med-shadow-18-white</font></param>
								</map></param>
								<param name="classes"><map/></param>
								<param name="images"><map/></param>
							</theme>
						</theme>
						<theme name="scrollpane">
              <param name="scrollbarsAlwaysVisible"><bool>false</bool></param>
							<param name="background"><image>none</image></param>
							<param name="overlay"><image>none</image></param>
							<theme name="hscrollbar" ref="hscrollbar"/>  <!-- not used -->
							<theme name="vscrollbar" ref="vscrollbar-white"/>
							<param name="border"><border>0,0,65,0</border></param>
							<param name="minWidth"><int>100</int></param>
							<param name="minHeight"><int>100</int></param>
							<param name="maxWidth"><int>32767</int></param>
							<param name="maxHeight"><int>32767</int></param>
							<param name="hasDragButton"><bool>false</bool></param>
							<param name="autoScrollArea"><int>5</int></param>
							<param name="autoScrollSpeed"><int>10</int></param>

							<theme name="dialoglayout" ref="-defaults">
								<param name="background"><image>none</image></param>
								<param name="border"><border>9,2,0,0</border></param>
								<param name="defaultGap"><dimension>3,3</dimension></param>
								<param name="smallGap"><dimension>8,8</dimension></param>
								<param name="mediumGap"><dimension>10,10</dimension></param>
								<param name="largeGap"><dimension>15,15</dimension></param>
								<param name="namedGaps"><map/></param>
								<param name="minWidth"><int>300</int></param>
								<param name="maxWidth"><int>300</int></param>
								<theme name="button" ref="mobile-bottom-button" allowWildcard="true">
									<param name="minWidth"><int>100</int></param>
									<param name="width"><int>100</int></param>
									<param name="border"><border>5,0</border></param>
									<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
								</theme>

								<theme name="label-title-icon" ref="label" allowWildcard="true">
									<param name="border"><border>1,5,0,5</border></param>
									<param name="minWidth"><int>24</int></param>
									<param name="font"><font>med-noshadow-18-white</font></param>
								</theme>
								<theme name="label-title-small" ref="label" allowWildcard="true">
									<param name="background"><image>ui-header.background</image></param>
									<param name="textAlignment"><enum type="alignment">left</enum></param>
									<param name="border"><border>6</border></param>
									<param name="minWidth"><int>100</int></param>
									<param name="font"><font>med-noshadow-18-white</font></param>
								</theme>
								<theme name="label-title-small2" ref="label" allowWildcard="true">
									<param name="background"><image>ui-header.background</image></param>
									<param name="textAlignment"><enum type="alignment">left</enum></param>
									<param name="border"><border>6</border></param>
									<param name="minWidth"><int>100</int></param>
									<param name="font"><font>med-noshadow-18-white</font></param>
								</theme>

								<theme name="label-title-small3" ref="label" allowWildcard="true">
									<param name="background"><image>ui-header.background</image></param>
									<param name="textAlignment"><enum type="alignment">left</enum></param>
									<param name="border"><border>1</border></param>
									<param name="minWidth"><int>90</int></param>
									<param name="font"><font>med-noshadow-18-white</font></param>
									<param name="textAlignment"><enum type="alignment">center</enum></param>
								</theme>

								<theme name="label-title-small4" ref="label" allowWildcard="true">
									<param name="background"><image>ui-header.background</image></param>
									<param name="textAlignment"><enum type="alignment">left</enum></param>
									<param name="border"><border>1</border></param>
									<param name="minWidth"><int>60</int></param>
									<param name="font"><font>med-noshadow-18-white</font></param>
									<param name="textAlignment"><enum type="alignment">center</enum></param>
								</theme>

								<theme name="label-value-icon" ref="label-title-icon" allowWildcard="true">
									<param name="background"><image>mobile.box.dark3.50</image></param>
								</theme>
								<theme name="label-value-small" ref="label-title-small" allowWildcard="true">
									<param name="background"><image>mobile.box.dark3.50</image></param>
								</theme>
								<theme name="label-value-small2" ref="label-title-small2" allowWildcard="true">
									<param name="background"><image>mobile.box.dark3.50</image></param>
								</theme>
								<theme name="label-value-small3" ref="label-title-small3" allowWildcard="true">
									<param name="background"><image>mobile.box.dark3.50</image></param>
								</theme>
								<theme name="label-value-small4" ref="label-title-small3" allowWildcard="true">
									<param name="background"><image>mobile.box.dark3.50</image></param>
								</theme>

								<theme name="label-title-perm" ref="label" allowWildcard="true">
									<param name="background"><image>ui-header.background</image></param>
									<param name="border"><border>15</border></param>
									<param name="minHeight"><int>40</int></param>
									<param name="font"><font>med-noshadow-18-white</font></param>
									<param name="textAlignment"><enum type="alignment">center</enum></param>
								</theme>

								<theme name="label-title-perm-val" ref="label-title-perm">
								</theme>

								<theme name="editfield" ref="editfield">
									<param name="maxWidth"><int>30000</int></param>
								</theme>

                <theme name="label-guild-log" ref="label-value-small">
                </theme>

								<theme name="togglebutton" ref="togglebutton">
									<param name="background"><image>ui-checkbox-readonly.background</image></param>
                  <param name="minHeight"><int>60</int></param>
								</theme>
							</theme>
						</theme>
					</theme>
				</theme>
				<param name="inputMap"><inputMapDef>
					<action name="nextTab">CTRL TAB</action>
					<action name="prevTab">CTRL SHIFT TAB</action>
				</inputMapDef></param>
			</theme>
		</theme>
	</theme>

	<theme name="member-table" ref="mobile-table" overwrite="true">
		<param name="columnWidths">
			<map>
				<param name="0"><int>font.EM * 20</int></param>
				<param name="1"><int>font.EM * 8</int></param>
				<param name="2"><int>font.EM * 8</int></param>
			</map>
		</param>

	</theme>
</themes>
