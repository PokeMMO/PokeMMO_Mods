<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<theme name="adminframe" ref="-defaults" allowWildcard="true">
		<param name="background"><image>mobile.frame.background</image></param>
		<param name="titleAreaTop"><int>10</int></param>
		<param name="titleAreaLeft"><int>8</int></param>
		<param name="titleAreaRight"><int>-1</int></param>
		<param name="titleAreaBottom"><int>60</int></param>
		<param name="closeButtonX"><int>-70</int></param>
		<param name="closeButtonY"><int>6</int></param>
		<param name="border"><border>80,10,10,10</border></param>
		<theme name="title" ref="-defaults">
			<param name="textAlignment"><enum type="alignment">left</enum></param>
			<param name="font"><font>xxlarge</font></param>
		</theme>
		<theme name="closeButton" ref="-defaults">
			<param name="background"><image>mobile.icons.close</image></param>
			<param name="minWidth"><int>60</int></param>
			<param name="minHeight"><int>60</int></param>
		</theme>
	</theme>

	<theme name="tp-menu" ref="mobile-panel" overwrite="true">
		<param name="background">
			<image>mobile.window.onebar.background</image>
		</param>
		<theme name="tabbedpane" ref="base-tabbed-pane">
			<param name="tabPosition">
				<enum type="tabposition">LEFT</enum>
			</param>
			<param name="background">
				<image>none</image>
			</param>
			<param name="border">
				<border>60,0,0,0</border>
			</param>
			<theme name="tabbox" ref="mobile-tabbox-inset" overwrite="true"/>
			<theme name="container" ref="container">
				<theme name="scrollpane" ref="scrollpane">
					<theme name="label" ref="label">
						<param name="border">
							<border>10</border>
						</param>
						<param name="font">
							<font>med-noshadow-18-white</font>
						</param>
					</theme>
					<theme name="button" ref="button">
						<param name="minWidth">
							<int>210</int>
						</param>
					</theme>
				</theme>
			</theme>
		</theme>
	</theme>

  <theme name="tp-direction-widget" ref="resizableframe" allowWildcard="true">
    <param name="minWidth">
      <int>220</int>
    </param>
    <param name="minHeight">
      <int>250</int>
    </param>
    <theme name="button-symbol" ref="button-symbol" allowWildcard="true">
      <param name="minWidth">
        <int>65</int>
      </param>
      <param name="minHeight">
        <int>65</int>
      </param>
    </theme>
  </theme>

  <theme name="gmpanel" ref="-defaults" allowWildcard="true">
		<theme name="button" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>button-tiny-blue.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>
		
		<theme name="button-search" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.dbsearch.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>
		
		<theme name="button-tools" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.tools.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>
		
		<theme name="button-player" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.psearch.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>
		
		<theme name="button-move" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.map.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>

		<theme name="button-sound" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.sound.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>

		<theme name="button-ping" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.lag.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>
		
		<theme name="button-support" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>26,0</border></param>
			<param name=""><image>mobile.icons.pchat.*</image></param>
			<param name="textAlignment"><enum type="alignment">LEFT</enum></param>
		</theme>
		
		
		<theme name="button-green" ref="-defaults" allowWildcard="true">
			<param name="font"><font>med-shadow-22-white</font></param>
			<param name="border"><border>0,0,0,0</border></param>
			<param name=""><image>mobile.icons.pchat.special.*</image></param>
			<param name="textAlignment"><enum type="alignment">BOTTOM</enum></param>
		</theme>
		
		<theme name="button-gc" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta</font></param>
			<param name="border"><border>7,0</border></param>
			<param name=""><image>mobile.icons.recyle.*</image></param>
			<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
		</theme>
		
		<theme name="admin-small-frame" ref="resizableframe">
			<param name="border"><border>15</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="button" ref="-defaults" allowWildcard="true">
				<param name="font"><font>alphabeta</font></param>
				<param name="border"><border>7,4</border></param>
				<param name=""><image>button-tiny.*</image></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
				<param name="minWidth"><int>140</int></param>
			</theme>
			<theme name="label-title" ref="label" allowWildcard="true">
				<param name="background"><image>label2.background</image></param>
				<param name="textAlignment"><enum type="alignment">left</enum></param>
				<param name="border"><border>5,5,5,5</border></param>
				<param name="minWidth"><int>250</int></param>
				<param name="font"><font>alphabeta</font></param>
			</theme>
			<theme name="dialoglayout" ref="-defaults">
				<param name="border"><border>15</border></param>
				<param name="defaultGap"><dimension>5,5</dimension></param>
				<param name="smallGap"><dimension>8,8</dimension></param>
				<param name="mediumGap"><dimension>10,10</dimension></param>
				<param name="largeGap"><dimension>15,15</dimension></param>
				<param name="namedGaps"><map/></param>
				<theme name="button" ref="-defaults" allowWildcard="true">
					<param name="font"><font>alphabeta</font></param>
					<param name="border"><border>7,4</border></param>
					<param name=""><image>button-tiny.*</image></param>
					<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
					<param name="minWidth"><int>140</int></param>
				</theme>
				<theme name="label-title" ref="label" allowWildcard="true">
					<param name="background"><image>label2.background</image></param>
					<param name="textAlignment"><enum type="alignment">left</enum></param>
					<param name="border"><border>5,5,5,5</border></param>
					<param name="minWidth"><int>250</int></param>
					<param name="font"><font>alphabeta</font></param>
				</theme>
			</theme>
		</theme>
	</theme>
</themes>